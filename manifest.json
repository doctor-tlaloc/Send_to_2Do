{

  "name": "Send to 2Do",
  "version": "1.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "128": "icon128.png"
  },
  "description": "A toolbar button to send the current page to 2Do.",
  "browser_action": {
    "default_icon": "browser_action.png",
    "default_title": "Send this page to 2Do"
  },
  "background": {
    "scripts" : ["options.js", "background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}
